        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.No)
            @Html.ValidationMessageFor(model => model.No)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.Amount)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Amount)
