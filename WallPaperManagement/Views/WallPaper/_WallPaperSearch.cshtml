@model System.Collections.Generic.List<WallPaperManagement.Models.WallPaper>


<table id="paperList" class="table table-striped table-hover ">
    <tr>
        <th>
            编号
        </th>
        <th>
            系列
        </th>
        <th>
            型号
        </th>
        <th>
            批次
        </th>
        <th>
            添加日期
        </th>
        <th>
            库存量
        </th>
        <th>
            操作
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @string.Format("{0:D8}",item.Id)
            </td> 
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SeriesName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                <input type="hidden" value="@item.Id"/>
               <input type="button" value="选择" onclick="setWallPaper(this)" class="btn"/>
            </td>
        </tr>
    }

</table>
